<% if @photos.count > 0 %>
  <br/><br/>

  <div class="row">
    <% @photos.each do |photo| %>
      <div class="col-md-12">
        <div class="panel panel-default">
          <div class="panel-heading preview text-center">
            <%= image_tag photo.image.url, class: "img-responsive img-thumbnail img-circle", id: "photo_#{photo.id}" %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
<% end %> 
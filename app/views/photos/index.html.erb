<p id="notice"><%= notice %></p>

<h1>Photos</h1>

<table>
  <thead>
    <tr>
      <th>Category</th>
      <th>Image</th>
      <th>Longitude</th>
      <th>Altitude</th>
      <th>Latitude</th>
      <th>Photoable type</th>
      <th>Photoable</th>
      <th>Is printable</th>
      <th>Photo datetime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <td><%= photo.category %></td>
        <td><%= photo.image %></td>
        <td><%= photo.longitude %></td>
        <td><%= photo.altitude %></td>
        <td><%= photo.latitude %></td>
        <td><%= photo.photoable_type %></td>
        <td><%= photo.photoable_id %></td>
        <td><%= photo.is_printable %></td>
        <td><%= photo.photo_datetime %></td>
        <td><%= link_to 'Show', photo %></td>
        <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
        <td><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photo', new_photo_path %>
